/**app.wxss**/
page {
  box-sizing: border-box;
  height: 100%;
}

.wrapper {
  width: 100%;
  min-height: 100%;
  padding-bottom: 10rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
